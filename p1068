#include<iostream>
#include<cstdio>
#include<cmath>
using namespace std;

typedef pair<int,int> PII;

const int N = 5000;

int q[N],s[N]; 

int main()
{
	int n,m;
	cin>>n>>m;
	
	for(int i = 0 ;i < n;i++)
	{
		cin>>q[i]>>s[i];
	}
	
	for(int i = 0;i < n - 1;i++)
	{
		for(int j = i + 1;j < n;j++)
		{
			if(s[i] < s[j]) 
			{
				swap(s[j],s[i]);
				swap(q[j],q[i]);
			}
			if(s[j] == s[i])
			{
				if(q[j] < q[i])
				{
					swap(q[j],q[i]);
				} 
			}
		}
	}
	
	int k = int(m * 1.5);
    int t = 0;
	int ddl = s[k - 1];
	
	for(int i = 0;i < n;i++)
	{
		if(s[i] >= ddl)
		{
			++t;
		}
		else break;
	}
	cout<<ddl<<" "<<t<<endl;
    for(int i = 0;i < t;i++)
    {
       printf("%d %d\n",q[i],s[i]);
    }
    
	return 0;
}
