#include<bits/stdc++.h>

using namespace std;

string a,c;

long long int b,i,r;

int main()
{
    cin>>a>>b;   
    for (;i < a.length();i++) a[i] -= 48;   
    
    for (i = 0;i < a.length(); i++)
        c.push_back((r * 10 + a[i]) / b + 48),r = (r * 10 + a[i]) % b;  
        
    for (i = 0;c[0] == 48;i++) c.erase(c.begin(),c.begin() + 1);  
    
    cout<<c;   
    return 0;     
}
