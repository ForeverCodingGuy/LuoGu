#include<iostream>
#include<cmath>
using namespace std;

bool is_prime(int x)
{
	for(int i = 2;i <= sqrt(x);i++)
	{
		if(x % i == 0) return false;
	}
	return true;
}

int main()
{
	int n;
	cin>>n;
	int cnt = 0;
	int num = 0;
	for(int i = 2; cnt <= n;i++)
	{
		if(is_prime(i))
		{
			cnt += i;
			if(cnt <= n)
			{
				cout<<i<<endl;
				num++;
			}
		}
	}
	cout<<num<<endl;
	return 0;
}
