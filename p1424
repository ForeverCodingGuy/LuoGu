#include<iostream>
using namespace std;

int main()
{
     int x,n;
     cin>>x>>n;
     unsigned long long int dis = 0;
     for(int i = 0;i < n;i++)
     {
         if((x + i)%7 != 6 && (x + i)%7!=0)
         { 
            dis += 250;
         }
     }
     cout<<dis<<endl;
     return 0;
}
            
