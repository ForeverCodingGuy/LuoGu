#include<iostream>
#include<algorithm>
#include<cstdio>
using namespace std;

int main()
{
	string s;
	cin>>s;
	while(s.size() > 1 && s[s.size() - 1] == '0') s.erase(s.end() - 1);
	if(s[0] == '-')
	{
		reverse(s.begin() + 1,s.end());
	}
	else reverse(s.begin(),s.end());
	for(int i = 0;i < s.size();i++) printf("%c",s[i]);
	return 0; 
}
