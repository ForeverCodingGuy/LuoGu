#include<iostream>
#include<cstdio>
using namespace std;

const int N = 100;

int q[N],n,idx;
bool st[1000];

void qsort(int* q,int l,int r)
{
    if(l >= r) return ;
    int i = l - 1,j = r + 1,mid = q[l + r >>1];

    while(i < j)
    {
        do i++; while(q[i] < mid);
        do j--; while(q[j] > mid);
        if(i < j) swap(q[i],q[j]);
    }

    qsort(q,l,j);
    qsort(q,j + 1,r);
    return ;
}

int main()
{
	cin>>n;
	
	for(int i = 0;i < n;i++)
	{
		int x;
		scanf("%d",&x);
		if(st[x]) continue;
		q[idx++] = x;
		st[x] = true;
	}
	
	qsort(q,0,idx - 1);
	
    cout<<idx<<endl;
	for(int i = 0;i < idx;i++) printf("%d ",q[i]);
	
	return 0;
}
