#include<iostream>
#include<cstdio>
using namespace std;

const int N = 100;

string name[N];
int ls[N],ps[N],pg[N],cnt[N],alls;
char west[N],mag[N];


int main()
{
	int n;
	cin>>n;
	
	for(int i = 0;i < n;i++)
	{
		cin>>name[i]>>ls[i]>>ps[i]>>mag[i]>>west[i]>>pg[i];
		if(ls[i] > 80 && pg[i] >= 1) cnt[i] += 8000;
		if(ls[i] > 85 && ps[i] > 80) cnt[i] += 4000;
		if(ls[i] > 90) cnt[i] += 2000;
		if(west[i] == 'Y' && ls[i] > 85) cnt[i] += 1000;
		if(mag[i] == 'Y' && ps[i] > 80) cnt[i] += 850;
		alls += cnt[i]; 
	} 
	
	
	string ans;
	int max_sum = 0;
	for(int i = 0;i < n;i++)
	{
		if(cnt[i] > max_sum)
		{
			ans = name[i];
			max_sum = cnt[i];
		}
	}
	
	cout<<ans<<endl;
	cout<<max_sum<<endl;
	cout<<alls<<endl;
	
	return 0;
}
