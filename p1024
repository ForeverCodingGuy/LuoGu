#include<iostream>
#include<cstdio>

using namespace std;

double a,b,c,d;

double mix(double x)
{
    return a*x*x*x+b*x*x+c*x+d;
}

int main()
{
    double l,r,mid,x1,x2;
    int cnt=0;
    scanf("%lf%lf%lf%lf",&a,&b,&c,&d);  //输入
    for (int i=-100;i<100;i++)
    {
        l=i; 
        r=i+1;
        x1=mix(l); 
        x2=mix(r);
        if(!x1) 
        {
            printf("%.2lf ",l); 
            cnt++;
        }  
        if(x1*x2<0)                             
        {
            while(r-l >= 1e-3)                    
            {
                mid = (l+r)/2;  
                if(mix(mid) * mix(r) <= 0) 
                   l = mid; 
                else 
                   r = mid;   
            }
            printf("%.2lf ",r);  
            cnt++;
        }
        if (cnt==3) break;             
    }
    return 0;
}
