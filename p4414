#include<iostream>
#include<map>
using namespace std;
int main()
{
     map<char,int> num;
     int a,b,c;
     cin>>a>>b>>c;
     if(a>b) swap(a,b);
     if(a>c) swap(a,c);
     if(b>c) swap(b,c);
     num.insert(pair<char,int>('A',a));
     num.insert(pair<char,int>('B',b));
     num.insert(pair<char,int>('C',c));
     char op[3];
     for(int i = 0;i < 3;i++)
     {
        cin>>op[i];
        cout<<num[op[i]]<<" ";
     }
     return 0;
}
