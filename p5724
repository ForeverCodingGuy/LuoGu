#include<iostream>
#include<limits.h>
#include<cstdio>
using namespace std;

const int N = 100;
int a[N];

int main()
{
	int n;
	cin>>n;
	int min_ = INT_MAX;
	int max_ = INT_MIN;
	for(int i = 0;i < n;i++)
	{
		scanf("%d",&a[i]);
		if(a[i] > max_) max_ = a[i];
		if(a[i] < min_) min_ = a[i];
	}
	cout<<max_ - min_;
	return 0; 
}
