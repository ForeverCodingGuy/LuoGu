#include<iostream>
#include<cstdio>
using namespace std;

double a[48];

double fab(int n)
{
    if(a[n] != 0) return a[n];
	if(n == 1 || n == 2) return a[n] = 1.0;
	if(n == 3) return a[n] = 2.0;
	return a[n] = fab(n - 1) + fab(n - 2);
}

int main()
{
	int n;
	scanf("%d",&n);
	printf("%.2lf",fab(n));
}
